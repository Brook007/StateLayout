<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StateLayout">
        <attr name="default_state" format="enum">
            <enum name="empty" value="0" />
            <enum name="loading" value="1" />
            <enum name="data" value="2" />
            <enum name="error" value="3" />
            <enum name="no_network" value="4" />
        </attr>
        <attr name="noDataView" format="reference" />
        <attr name="onLoadingView" format="reference" />
        <attr name="onErrorView" format="reference" />
        <attr name="onNoNetworkView" format="reference" />
    </declare-styleable>

    <declare-styleable name="StateLayout_Layout">
        <attr name="view_state" format="enum">
            <enum name="empty" value="0" />
            <enum name="loading" value="1" />
            <enum name="data" value="2" />
            <enum name="error" value="3" />
            <enum name="no_network" value="4" />
        </attr>
    </declare-styleable>
</resources>